{
  "id": "vscode-default",
  "name": "VS Code Default",
  "description": "Your current VS Code setup - exactly as you have it configured",
  "tags": ["default", "original", "personal", "current"],
  "author": "Your Setup",
  "version": "1.0.0",
  "extensions": [],
  "settings": {
    "code-runner.runInTerminal": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "github.copilot.editor.enableAutoCompletions": true,
    "explorer.confirmDelete": false,
    "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "git.autofetch": true,
    "github.copilot.preferredAccount": "KhoaWorkHub",
    "diffEditor.hideUnchangedRegions.enabled": true,
    "editor.fontSize": 14,
    "typescript.preferences.preferTypeOnlyAutoImports": true,
    "editor.foldingImportsByDefault": true,
    "javascript.experimental.updateImportsOnPaste": true,
    "[javascriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.statusBar.visible": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "[json]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "github.copilot.nextEditSuggestions.enabled": true,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "git.confirmSync": false,
    "gitlens.ai.model": "vscode",
    "gitlens.ai.vscode.model": "copilot:gpt-4.1"
  }
}
