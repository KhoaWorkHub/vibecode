{
  "id": "vscode-default",
  "name": "VS Code Default",
  "description": "Your current VS Code setup - exactly as you have it configured",
  "tags": ["default", "original", "personal", "current"],
  "author": "Your Setup",
  "version": "1.0.0",
  "extensions": [],
  "settings": {
    "code-runner.runInTerminal": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "github.copilot.editor.enableAutoCompletions": true,
    "explorer.confirmDelete": false,
    "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "git.autofetch": true,
    "github.copilot.preferredAccount": "KhoaWorkHub",
    "diffEditor.hideUnchangedRegions.enabled": true,
    "editor.fontSize": 14,
    "typescript.preferences.preferTypeOnlyAutoImports": true,
    "editor.foldingImportsByDefault": true,
    "javascript.experimental.updateImportsOnPaste": true,
    "[javascriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.statusBar.visible": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "[json]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "github.copilot.nextEditSuggestions.enabled": true,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "git.confirmSync": false,
    "gitlens.ai.model": "vscode",
    "gitlens.ai.vscode.model": "copilot:gpt-4.1",
    "workbench.colorTheme": "Default Light Modern",
    "workbench.iconTheme": "vs-seti",
    "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
    "editor.lineHeight": 0,
    "editor.letterSpacing": 0,
    "editor.fontWeight": "normal",
    "editor.cursorStyle": "line",
    "editor.cursorBlinking": "blink",
    "editor.renderLineHighlight": "line",
    "editor.lineNumbers": "on",
    "editor.minimap.enabled": true,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.renderWhitespace": "selection",
    "editor.wordWrap": "off",
    "editor.rulers": [],
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
    "terminal.integrated.lineHeight": 1,
    "window.zoomLevel": 0,
    "workbench.activityBar.location": "default",
    "workbench.sideBar.location": "left",
    "breadcrumbs.enabled": true,
    "explorer.compactFolders": true,
    "files.autoSave": "off",
    "files.trimTrailingWhitespace": false
  }
}
